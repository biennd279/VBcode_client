\n            localStorage.removeItem('user');\n            state.isLoggedIn = false;\n            state.currentUser = null;\n        },\n    },\n    actions: {\n        LOGIN: (context) => {\n            context.commit('LOGIN');\n            console.log('Log in');\n        },\n    }\n}"]}]}